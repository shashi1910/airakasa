{
  "version": 2,
  "installCommand": "npm run install:all",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "app/frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

